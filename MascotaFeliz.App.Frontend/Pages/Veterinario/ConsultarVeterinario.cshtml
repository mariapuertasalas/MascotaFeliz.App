
<section class='general'>
  <div class='contenedor-titulo'>
    <h4>Detalle del Veterinario</h4>
  </div>
  <div class='formulario' id='register-veterinario'>
    <input type='hidden' name='id' id='id' value='@Model.veterinario.Id'>
    <input type='text' name='identificacion' id='identificacion' class='form-veterinario' placeholder='Ingrese Identificacion'value= '@Model.dueno.Identificacion'>
    <input type='text' name='nombres' id='nombres' class='form-veterinario' placeholder=' Nombres'value= '@Model.dueno.Nombres' disabled>
    <input type='text' name='apellidos' id='apellidos' class='form-veterinario' placeholder='Apellidos'value= '@Model.dueno.Apellidos'disabled>
    <input type='text' name='direccion' id='direccion' class='form-veterinario' placeholder=' Direccion'value= '@Model.dueno.Direccion'disabled>
    <input type='tel' name='telefono' id='telefono' class='form-veterinario' placeholder='Telefono'value= '@Model.dueno.Telefono'disabled>
    <input type='number' name='tarjeta_profesional' id='tarjeta_profesional' class='form-veterinario' placeholder=' Tarjeta Profesional'value= '@Model.dueno.Tarjeta Profesional'disabled> 
  </div>
</section>